<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<VBox alignment="CENTER" prefHeight="322.0" prefWidth="313.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.2" fx:controller="ru.vsu.cs.novikov.main.HelloApplication">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <ChoiceBox  fx:id="choiceBox" layoutX="16.0" layoutY="52.0" prefWidth="150.0" value="2 игрока">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="2 игрока" />
                <String fx:value="4 игрока" />
                <String fx:value="6 игроков" />
            </FXCollections>
        </items>
    </ChoiceBox>

    <Button onAction="#onHelloButtonClick" text="Играть!" />

</VBox>
